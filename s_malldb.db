USE s_malldb; /*s_malldb를 사용하겠다*/
show tables;/* 테이블 보는 명령어*/

/*테이블 생성*/
CREATE TABLE product (
  p_name VARCHAR(32) NOT NULL,
  price INT(11) NULL,
  image BLOB NULL,
  info VARCHAR(100) NULL,  
  PRIMARY KEY (p_name)
);

CREATE TABLE notice(
title varchar(100) not NULL,
content varchar(100) NULL,
upload_date DATE NULL,
PRIMARY KEY (title)
);

CREATE TABLE faq(
title varchar(100) not NULL,
content varchar(100) NULL,
upload_date DATE NULL,
PRIMARY KEY (title)
);
